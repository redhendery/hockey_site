<div class="card">
  <div class="card-header">
    <strong>Team SVS Leaders</strong>
  </div>
  <div class="card-block">
    <% @svs.each_with_index do |svs, i| %>
      <div class="table-responsive">
        <span><strong><%= i += 1 %>. </strong></span>
        <span><strong><%= svs.name %></strong></span>
        <span class="float-right"><strong>.<%= svs.svs %></strong></span>
      </div>
    <% end %>
  </div>
</div>
